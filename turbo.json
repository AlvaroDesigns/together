{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "outputs": [
                "dist/**"
            ],
            "dependsOn": [
                "^build"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "deploy": {
            "dependsOn": [
                "build"
            ]
        },
        "clean": {
            "cache": false
        }
    }
}